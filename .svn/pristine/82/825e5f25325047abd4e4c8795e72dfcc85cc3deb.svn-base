body {
	  background-color: rgba(217, 244, 255, 0.01);
	  text-align: center;
}

.center {
	text-align: center;
}

.logoContain{
	margin: 13px;
	width: 90px;
	height: 90px;
	border-radius: 150px;
	border: 3px solid #f0f0f1;
	-webkit-border-radius: 260px;
	-moz-border-radius: 260px;
	background: url(/images/RaTunesCover.png) no-repeat;
	background-size: 100% 100%;
	display: block;
	float: left;
	padding: 5px;
}

.menu{
	padding: 25px;
	background-color: #333333;
	float: left;
	width: 30%;
	min-height: 100%;
	/*border-right: 1px solid;*/
	/*border-right-color: #333333;*/
}

.albumsButton{
	color: #070101;
	border-radius: 12px;
	border: 2px solid #f0f0f1;
	background-color: #f0f0f1;

}

li {
	float: left;
}

a {
	display: block;
	width: 60px;
}

.-cx-PRIVATE-Shell__nav {
	background-image: linear-gradient( #333333 100%, transparent 50%);
	height: 120px;
	border-bottom-width: 1px;
	-webkit-box-ordinal-group: 2;
	-webkit-order: 1;
	-ms-flex-order: 1;
	order: 1;
	padding: 0 20px;
	/*border-bottom: 1px solid;*/
	/*border-bottom-color: #333333;*/
}

.right {
	float: right;
}

.task{
	background-color: #73ad21;
	height: 60px;
	/*border-bottom: 1px solid;*/
	/*border-bottom-color: #333333;*/
}

.menuContent{
	background-color: #6783b2;
	width: auto;
	height: auto;
	/*border-bottom: 1px solid;*/
	/*border-bottom-color: #333333;*/
}

.logoutButton{
	margin: 10px;
	width: auto;
	color: #070101;
	border-radius: 12px;
	border: 2px solid #f0f0f1;
	background-color: #f0f0f1;
}

.italic{
	font-style: italic;
	color: #73ad21;
}

.containContent{
	padding: 20px;
	background-color: #5b666b;
	width: auto;
	height: 100%;
}

input:hover {
	/*background: #8FBC8F;*/
