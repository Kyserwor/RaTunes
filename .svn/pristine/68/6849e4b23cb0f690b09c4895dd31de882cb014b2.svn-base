.right {
	float: none;
	margin-left: auto;
	margin-right: auto;
	width: 500px;
	/**border:3px solid #73AD21;**/
	padding: 40px;
}

body {
	  background-color: rgba(217, 244, 255, 0.01);
	  text-align: center;
}

.center {
	text-align: center;
}

.logoContain{
	margin: 13px;
	width: 120px;
	height: 120px;
	border-radius: 150px;
	border: 3px solid #f0f0f1;
	-webkit-border-radius: 260px;
	-moz-border-radius: 260px;
	background: url(/images/RaTunesCover.png) no-repeat;
	background-size: 100% 100%;
	display: block;
	float: left;
	padding: 10px;
}

.menu{
	padding: 10px;
	background-color: #425b84;
	float: left;
	width: 400px;
	height: 900px;
	border-right: 1px solid;
	border-right-color: #333333;
}

.albumsButton{
	color: #070101;
	border-radius: 12px;
	border: 1px solid #787878;
	background-color: #f0f0f1;

}

li {
	float: left;
}

a {
	display: block;
	width: 60px;
}

.-cx-PRIVATE-Shell__nav {
	background-image: linear-gradient( #425b84 100%, transparent 50%);
	height: 140px;
	border-bottom-width: 1px;
	-webkit-box-ordinal-group: 2;
	-webkit-order: 1;
	-ms-flex-order: 1;
	order: 1;
	padding: 0 20px;
	border-bottom: 1px solid;
	border-bottom-color: #333333;
}

.right {
	float: right;
}

.task{
	background-color: #546f9d;
	height: 60px;
	border-bottom: 1px solid;
	border-bottom-color: #333333;
}

.menuContent{
	background-color: #6783b2;
	width: auto;
	height: auto;
	border-bottom: 1px solid;
	border-bottom-color: #333333;
}

.this{
	border-color: green;
	width: auto;
	border-style: solid;
}

.logoutButton{
	margin: auto;
	color: #070101;
	border-radius: 12px;
	border: 1px solid #787878;
	background-color: #f0f0f1;
}