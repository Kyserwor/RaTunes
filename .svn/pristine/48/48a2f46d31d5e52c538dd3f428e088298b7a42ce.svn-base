public class Artist {
    private int id;
    private String name;

    public Artist(){
        this.id = 0;
        this.name = null;
    }

    public Artist(int id, String name) {
        this.id = id;
        this.name = name;
    }

    //region Getter and Setter
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }
    //endregion
}
